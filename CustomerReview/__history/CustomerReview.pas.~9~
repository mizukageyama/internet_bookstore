unit CustomerReview;

interface

uses
  MVCFramework.Serializer.Commons,
  MVCFramework.ActiveRecord,
  MVCFramework.Nullables,
  MVCFramework.Commons,
  MVCFramework.SQLGenerators.MySQL,
  System.Generics.Collections;

type
  [MVCNameCase(ncCamelCase)]
  [MVCTable('customer_review')]
  TBook = class(TMVCActiveRecord)
  private
    [MVCTableField('ID', [foPrimaryKey, foAutoGenerated])]
    FId: Integer;
    [MVCTableField('CUSTOMER_ID')]
    FCustomerId: string;
    [MVCTableField('BOOK_ID')]
    FBookId: string;
    [MVCTableField('REVIEW')]
    FReview: string;
    [MVCTableField('FIRST_NAME')]
    FRating: string;
    [MVCTableField('FIRST_NAME')]
    FTitle: string;

  public
    function IsValid: Boolean;
    property Id: Integer read FId write FId;
    property BookId: string read FBookId write FBookId;
    property CustomerId: string read FCustomerId write FCustomerId;
    property Title: string read FTitle write FTitle;
    property Review: string read FReview write FReview;
    property Rating: string read FRating write FRating;
  end;

implementation



{ TBook }

function TBook.IsValid: Boolean;
begin

end;

end.

